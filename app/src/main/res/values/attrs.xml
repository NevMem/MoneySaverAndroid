<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BarChart">
        <attr name="amountOfBars" format="integer" />
        <attr name="rounded" format="boolean" />
        <attr name="barWidth" format="integer" />
        <attr name="baseColor" format="color" />
        <attr name="labelsFontColor" format="color" />
        <attr name="showLabels" format="boolean" />
        <attr name="labelsLiningColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PieChart">
        <attr name="chartPadding" format="float" />
    </declare-styleable>
</resources>